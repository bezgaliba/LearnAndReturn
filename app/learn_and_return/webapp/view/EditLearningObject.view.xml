<mvc:View
    controllerName="learnandreturn.controller.EditLearningObject"
    displayBlock="true"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc">
        <mvc:View
    controllerName="learnandreturn.controller.AddLearningObject"
    displayBlock="true"
    xmlns="sap.m"
    xmlns:f="sap.ui.layout.form"
    xmlns:core="sap.ui.core"
    xmlns:mvc="sap.ui.core.mvc">
    <Button text="{i18n>navBackButtonTitle}" press=".onNavBack"></Button>
     <f:SimpleForm id="simpleForm" title="{i18n>addLearningObjectHeader}" editable="true" class="editableForm sapUiSmallMarginTopBottom">
      <f:content>
       <Label text="{i18n>addLearningObjectName}"/>
       <Input
          id="formLearningObjectName"
          required="true"
          placeholder="..."/>
       <Label text="{i18n>addLearningObjectType}"/>
       <Select
            id="formLearningObjectType"
            required="true"
            items="{
               path: '/LearningObjectType'
               }">
          <core:Item key="{ID}" text="{name}" />
       </Select>
       <Label text="{i18n>addLearningObjectContent}"/>
       <Input
          id="formLearningObjectContent"
          required="true"
          placeholder="..."/>
       <Label text="{i18n>formLearningObjectDescription}"/>
       <Input 
          height="500px"
          id="formLearningObjectDescription"
          placeholder="..." />
      </f:content>
     </f:SimpleForm>
     <Button text="{i18n>editAction}" press="onEdit"></Button>
</mvc:View>
</mvc:View>